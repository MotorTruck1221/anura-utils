{
    "name": "create-anura-app",
    "version": "2.0.0",
    "description": "Quickly scaffold an anura application",
    "type": "module",
    "bin": {
        "create-anura-app": "dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MotorTruck1221/create-anura-app.git"
    },
    "scripts": {
        "format": "biome format ./ --write && biome check --apply .",
        "types": "tsc --noEmit",
        "build": "npm run types && babel src/ --out-dir dist/ --extensions '.ts' --copy-files",
        "release": "npm publish --access public"
    },
    "keywords": [
        "template",
        "anura",
        "anuraOS"
    ],
    "contributors": [
        "MotorTruck1221 (https://github.com/motortruck1221)",
        "Endercass (https://github.com/endercass)",
        "Percs (https://github.com/percslol)"
    ],
    "bugs": {
        "url": "https://github.com/motortruck1221/create-anura-app/issues"
    },
    "homepage": "https://github.com/motortruck1221/create-anura-app",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.24.5",
        "@babel/core": "^7.24.5",
        "@babel/plugin-syntax-import-attributes": "^7.24.1",
        "@babel/preset-env": "^7.24.5",
        "@babel/preset-typescript": "^7.24.1",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.16.2",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@clack/core": "^0.3.4",
        "@clack/prompts": "^0.7.0",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "execa": "^9.1.0",
        "fs-extra": "^11.2.0",
        "giget": "^1.2.3",
        "sort-package-json": "^2.10.0"
    }
}
